{
    "name": "elgoumri-oussama/php-uptobox",
    "description": "Command line tool for uptobox.com built for personal use only!",
    "license": "MIT",
    "require": {
        "elgoumri-oussama/php-curl": "^0.1.0",
        "elgoumri-oussama/php-random-user-agent": "^0.1.2",
        "vlucas/phpdotenv": "^2.4",
        "kevinlebrun/colors.php": "^1.0",
        "symfony/console": "^3.2",
        "elgoumri-oussama/php-base-path": "^0.0.1",
        "symfony/dom-crawler": "^3.2",
        "symfony/css-selector": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "OussamaElgoumri\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OussamaElgoumri\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "elgoumri-oussama/php-test-common": "^0.0.4"
    },
    "bin": [
        "uptobox"
    ]
}
